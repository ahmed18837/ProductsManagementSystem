    "schemas": {
      "AddUserDto": {
        "required": [
          "confirmPassword",
          "email",
          "fullName",
          "password",
          "phoneNumber",
          "role"
        ],
        "type": "object",
        "properties": {
          "fullName": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string",
            "default": "FullName"
          },
          "email": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string",
            "format": "email",
            "default": "user@example.com"
          },
          "phoneNumber": {
            "minLength": 1,
            "type": "string",
            "format": "tel",
            "default": "01234567890"
          },
          "password": {
            "maxLength": 64,
            "minLength": 8,
            "type": "string",
            "format": "password",
            "default": "SecurePass123"
          },
          "confirmPassword": {
            "maxLength": 64,
            "minLength": 8,
            "type": "string",
            "format": "password",
            "default": "SecurePass123"
          },
          "role": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "AssignRoleDto": {
        "required": [
          "email",
          "role"
        ],
        "type": "object",
        "properties": {
          "email": {
            "maxLength": 100,
            "minLength": 1,
