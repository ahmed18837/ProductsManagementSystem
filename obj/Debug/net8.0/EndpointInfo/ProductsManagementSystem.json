            "format": "email",
            "default": "user@example.com"
          },
          "role": {
            "minLength": 1,
            "type": "string",
            "default": "User"
          }
        },
        "additionalProperties": false
      },
      "ChangePasswordDto": {
        "required": [
          "currentPassword",
          "email",
          "newPassword"
        ],
        "type": "object",
        "properties": {
          "email": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string",
            "format": "email",
            "default": "user@example.com"
          },
          "currentPassword": {
            "maxLength": 64,
            "minLength": 8,
            "type": "string",
            "format": "password",
            "default": "SecurePass123"
          },
          "newPassword": {
            "maxLength": 64,
